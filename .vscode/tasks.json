{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build active file",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "${fileDirname}/TruthTable/TruthTable.cpp",
                "${fileDirname}/KnownResult/KnownResult.cpp",
                "${fileDirname}/CustomError/CustomError.cpp",
                "${fileDirname}/Helpers/Helpers.cpp",
                "${fileDirname}/Perceptron/Perceptron.cpp",
                "-o",
                "${fileDirname}/../out/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}